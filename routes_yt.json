{
  "dns": {
    "rules": [
      {
        "domain_suffix": [
          "appcenter.ms",
          "firebase.io",
          "crashlytics.com"
        ],
        "geosite": "category-ads-all",
        "server": "dns-block"
      },
      {
        "domain_suffix": [
          "storage.googleapis.com"
        ],
        "geosite": "youtube",
        "server": "dns-remote"
      },
      {
        "domain_suffix": [
          "private"
        ],
        "geoip": [
          "ru",
          "private"
        ],
        "geosite": [
          "category-ru",
          "google",
          "telegram",
          "github",
          "reddit",
          "twitch"
        ],
        "server": "dns-local"
      }
    ],
    "final": "dns-remote"
  },
  "route": {
    "rules": [
      {
        "domain_suffix": [
          "appcenter.ms",
          "firebase.io",
          "crashlytics.com"
        ],
        "geosite": "category-ads-all",
        "outbound": "block"
      },
      {
        "domain_suffix": [
          "storage.googleapis.com"
        ],
        "geosite": "youtube",
        "outbound": "proxy"
      },
      {
        "domain_suffix": [
          "private"
        ],
        "geoip": [
          "ru",
          "private"
        ],
        "geosite": [
          "category-ru",
          "google",
          "telegram",
          "github",
          "reddit",
          "twitch"
        ],
        "outbound": "direct"
      }
    ],
    "final": "proxy"
  }
}
